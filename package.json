{
  "name": "playwright-taf",
  "version": "1.0.0",
  "license": "ISC",
  "description": "SDET Mastery JS",
  "author": "Vladislav Sheverda",
  "keywords": [
    "playwright",
    "node",
    "javascript",
    "api",
    "ui",
    "taf"
  ],
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladsheverda7/JSMastery"
  },
  "scripts": {
    "test": "jest",
    "test:unit": "jest test.frameworks/trafficLight.test.js",
    "test:login": "npx playwright test tests/ui.tests/login/.*.spec.js --project=chromium",
    "test:purchase": "npx playwright test tests/ui.tests/purchase/.*.spec.js --project=chromium",
    "test:e2e": "npx playwright test --project=chromium",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --project=chromium --headed",
    "report": "npx playwright show-report",
    "create:db": "node database/scripts/create.db.js",
    "create:AuthorTable": "node database/scripts/authorTable/create.authorTable.js",
    "insert:authors": "node database/scripts/authorTable/insert.authorData.js",
    "delete:authors": "node database/scripts/authorTable/delete.authorData.js",
    "drop:authors": "node database/scripts/authorTable/drop.authorTable.js",
    "create:bookTable": "node database/scripts/bookTable/create.bookTable.js",
    "insert:books": "node database/scripts/bookTable/insert.booksData.js",
    "delete:books": "node database/scripts/bookTable/delete.bookData.js",
    "drop:books": "node database/scripts/bookTable/drop.bookTable.js",
    "create:bookDetailsTable": "node database/scripts/bookDetails/create.BookDetails.js",
    "insert:booksDetails": "node database/scripts/bookDetails/insert.BookDetailData.js",
    "delete:booksDetails": "node database/scripts/bookDetails/delete.bookDetails.js",
    "test:checkData": "mocha database/tests/test.js",
    "test:db_creation": "mocha database/tests/database.tests.js",
    "create:tables": "node database/scripts/authorTable/create.authorTable.js && node database/scripts/bookTable/create.bookTable.js && node database/scripts/bookDetails/create.BookDetails.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.0",
    "chai": "^4.3.8",
    "jest": "^29.6.2",
    "mocha": "^10.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1"
  }
}